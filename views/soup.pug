extends layout
block content
  h1= title
  -let results=[{soup_name:"Tomato",soup_size:"small",price:4.56},{soup_name:"Corn",soup_size:"medium",price:6.72},{soup_name:"Chicken",soup_size:"large",price:9.92}]
  div(class="container-fluid")
  div(class="row")
    -
    each item in results 
        div(class="col-sm-2") #{item.soup_name} #{item.soup_size} #{item.price}